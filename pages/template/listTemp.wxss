/* pages/template/listTemp.wxss */
.tmpContainer {
  display: flex;
  flex-direction: column;
  border-top: 1rpx solid #eee;
  border-bottom: 1rpx solid #eee;
}

.pubDate image {
  width: 60rpx;
  height: 60rpx;
  vertical-align: middle;
  padding: 10rpx;
}
.pubDate text {
  margin-left: 10rpx;
  font-size: 28rpx;
}
.company {
  font-size: 36rpx;
  font-weight: 700;
  margin-left: 10rpx;
}
.contentImg {
  width: 100%;
  height: 360rpx;
  margin: 10rpx 0;
}
.content {
  font-size: 32rpx;
  text-indent: 32rpx;
}
.collectionLove text {
  font-size: 28rpx;
  margin-left: 5rpx;
}
.collectionLove image {
  width: 30rpx;
  height: 30rpx; 
  margin-left: 10rpx;
  vertical-align: middle; 
}